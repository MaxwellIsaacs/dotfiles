" "  //wrapper function for determinant
" "  public double det () {
    if (this.isSquare()) return this.squareDeterminant();
    else return this.nonSquareDeterminant();

" "  //in place determinant
" "  public void squareDeterminant (){}
" "
  public static double squareDeterminant (Matrix x) {
    if (!Matrix.isSquare(x)){
      Error.incorrectMatrixShape (\"square determinant\");
      return 69;
    }

    if (x.getCols() == 2) {
      return determinant2d (x);
    }

    int n = x.getCols();
    int stepCount = n-2;
    double det = 0;
    Vector firstRow = x.grabRowVector (0);
    for (int i = 0; i < firstRow.getLength(); i++) {
      double pivot = firstRow.getElement (i);
      Matrix temp = new Matrix (n-1, n-1);
      for (int j = 1; j < x.getRows(); j++) {
        for (int k = 0; k < x.getCols(); k++) {
          if (k == i) continue;
          if (k > i) {
            temp.setElement (j-1, k-1, x.getElement (j,k));
          } else {
            temp.setElement (j-1, k, x.getElement (j, k));
          }
        }
      }
      det += Math.pow(-1, i) * pivot * Matrix.squareDeterminant (temp);
    }
    return det;
  }

